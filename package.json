{
  "name": "raco-dark-mode",
  "version": "2.0.0",
  "description": "I want to study, not to get blind",
  "author": "Nara Díaz Viñolas <me@rdvdev2.com>",
  "scripts": {
    "build": "sass --no-charset -I src/utils --no-source-map src/main.user.scss main.user.css",
    "clean": "del-cli main.user.css",
    "watch": "npm-watch"
  },
  "watch": {
    "build": {
      "patterns": ["src"],
      "extensions": "scss"
    }
  },
  "devDependencies": {
    "del-cli": "^5.0.0",
    "sass": "^1.58.3",
    "npm-watch": "^0.11.0"
  }
}
